{
  "remote.autoForwardPortsSource": "hybrid",
  "codeium.enableConfig": {
    "*": true,
    "git-commit": true
  },
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "vscode:gpt-4",
  "chat.agent.enabled": true,
  "anthropic.claude-code": {
    "enabled": true,
    "autoApply": false
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "github.copilot.chat.enabled": true,
  "chat.commandCenter.enabled": true,
  "editor.fontSize": 13,
  "editor.fontFamily": "Menlo, Monaco, 'Courier New'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
  "editor.rulers": [80, 100, 120],
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/dist": true,
    "**/build": true
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontSize": 12,
  "git.autofetch": true,
  "gitlens.showWelcomeOnInstall": false,
  "extensions.ignoreRecommendations": false,
  "typescript.surveys.enabled": false,
  "workbench.startupEditor": "none",
  "telemetry.telemetryLevel": "off",
  "breadcrumbs.enabled": true,
  "minimap.enabled": true,
  "aws.telemetry": false,
  "workbench.editor.enablePreview": false,
  "git.defaultCloneDirectory": "/Users/franksimpson/CascadeProjects",
  "workbench.projectManagement.autoOpenProjects": "prompt",
  "workbench.workspaceManagement.excludeViewGroups": [],
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/build/**": true
  }
}
